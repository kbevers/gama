#!/bin/sh

set -e

for g in @INPUT_FILES@
do
for a in envelope gso cholesky svd
do
    @top_builddir@/bin/gama-g3 --algorithm $a @G3_INPUT@/$g.xml \
       > @G3_RESULTS@/$g-$a.adj.xml

    scripts/check_adjustment @G3_INPUT@/$g-adj.xml @G3_RESULTS@/$g-$a.adj.xml
done
done


for g in dopnul vyberova_udrzba
do
    @top_builddir@/bin/gama-g3 --algorithm envelope \
                             @G3_INPUT@/sjtsk05/$g.xml > \
                             @G3_RESULTS@/$g-envelope.adj.xml

    scripts/check_adjustment @G3_INPUT@/sjtsk05/$g.adj.xml \
                             @G3_RESULTS@/$g-envelope.adj.xml
done
